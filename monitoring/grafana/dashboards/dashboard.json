          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH base_data AS (\n  SELECT\n    timestamp,\n    SQRT(x*x + y*y + z*z) as magnitude\n  FROM\n    accelerometer_data\n  WHERE\n    $__timeFilter(timestamp)\n),\nstats AS (\n  SELECT AVG(magnitude) as avg_magnitude, STDDEV(magnitude) as stddev_magnitude\n  FROM base_data\n)\nSELECT\n  DATE_TRUNC('second', timestamp) as time,\n  AVG(magnitude) as avg_magnitude,\n  AVG(ABS(magnitude - stats.avg_magnitude)) as avg_deviation,\n  AVG((magnitude - stats.avg_magnitude) / NULLIF(stats.stddev_magnitude, 0)) as avg_zscore\nFROM\n  base_data, stats\nGROUP BY\n  DATE_TRUNC('second', timestamp)\nORDER BY\n  time ASC",
          "refId": "A",

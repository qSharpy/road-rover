        {
          "datasource": {
            "type": "grafana-postgresql-datasource",
            "uid": "PCC52D03280B7034C"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH acceleration_data AS (\n  SELECT\n    timestamp,\n    x, y, z,\n    LAG(x) OVER (ORDER BY timestamp) as prev_x,\n    LAG(y) OVER (ORDER BY timestamp) as prev_y,\n    LAG(z) OVER (ORDER BY timestamp) as prev_z,\n    EXTRACT(EPOCH FROM (timestamp - LAG(timestamp) OVER (ORDER BY timestamp))) as time_diff\n  FROM\n    accelerometer_data\n  WHERE\n    $__timeFilter(timestamp)\n),\nrate_of_change AS (\n  SELECT\n    timestamp,\n    x, y, z,\n    (x - prev_x) / NULLIF(time_diff, 0) as x_rate,\n    (y - prev_y) / NULLIF(time_diff, 0) as y_rate,\n    (z - prev_z) / NULLIF(time_diff, 0) as z_rate\n  FROM\n    acceleration_data\n),\nstats AS (\n  SELECT\n    AVG(ABS(x_rate)) as avg_x_rate,\n    STDDEV(ABS(x_rate)) as stddev_x_rate,\n    AVG(ABS(y_rate)) as avg_y_rate,\n    STDDEV(ABS(y_rate)) as stddev_y_rate,\n    AVG(ABS(z_rate)) as avg_z_rate,\n    STDDEV(ABS(z_rate)) as stddev_z_rate\n  FROM\n    rate_of_change\n)\nSELECT\n  timestamp,\n  x, y, z,\n  x_rate, y_rate, z_rate,\n  (ABS(x_rate) - avg_x_rate) / NULLIF(stddev_x_rate, 0) as x_zscore,\n  (ABS(y_rate) - avg_y_rate) / NULLIF(stddev_y_rate, 0) as y_zscore,\n  (ABS(z_rate) - avg_z_rate) / NULLIF(stddev_z_rate, 0) as z_zscore,\n  CASE \n    WHEN (ABS(x_rate) - avg_x_rate) / NULLIF(stddev_x_rate, 0) > 3 OR\n         (ABS(y_rate) - avg_y_rate) / NULLIF(stddev_y_rate, 0) > 3 OR\n         (ABS(z_rate) - avg_z_rate) / NULLIF(stddev_z_rate, 0) > 3 \n    THEN 'Potential Pothole'\n    ELSE 'Normal'\n  END as pothole_indicator\nFROM\n  rate_of_change, stats\nWHERE\n  (ABS(x_rate) - avg_x_rate) / NULLIF(stddev_x_rate, 0) > 2 OR\n  (ABS(y_rate) - avg_y_rate) / NULLIF(stddev_y_rate, 0) > 2 OR\n  (ABS(z_rate) - avg_z_rate) / NULLIF(stddev_z_rate, 0) > 2\nORDER BY\n  timestamp ASC",

          "rawQuery": true,
          "rawSql": "WITH base_data AS (\n  SELECT\n    timestamp,\n    SQRT(x*x + y*y + z*z) as magnitude\n  FROM\n    accelerometer_data\n  WHERE\n    $__timeFilter(timestamp)\n)\nSELECT\n  DATE_TRUNC('second', timestamp) as time,\n  AVG(ABS(magnitude - 9.81)) as avg_deviation\nFROM\n  base_data\nGROUP BY\n  DATE_TRUNC('second', timestamp)\nORDER BY\n  time ASC",
          "refId": "A",
